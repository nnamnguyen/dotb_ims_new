<div class="howto quotes-config">
    <h3 id="howtoTitle">{{howtoData.title}}</h3>
    <div class="field-search">
        <input type="text" placeholder="{{str 'LBL_SEARCH'}}" class="searchbox">
    </div>
    <div class="legend">
        <h4>{{str 'LBL_CONFIG_LEGEND' 'Quotes'}}</h4>
        <div class="legend-inputs">
            <span class="span4 tristate-checkbox">
                <input type="checkbox" class="unchecked" onclick="return false;">
                <label class="checkbox-label">
                    {{str 'LBL_CONFIG_LEGEND_INPUT_UNCHECKED' 'Quotes'}}
                </label>
            </span>
            <span class="span4 tristate-checkbox">
                <input type="checkbox" class="checked" onclick="return false;" checked>
                <label class="checkbox-label">
                    {{str 'LBL_CONFIG_LEGEND_INPUT_CHECKED' 'Quotes'}}
                </label>
            </span>
            <span class="span4 tristate-checkbox">
                <input type="checkbox" class="indeterminate" onclick="return false;">
                <label class="checkbox-label">
                    {{str 'LBL_CONFIG_LEGEND_INPUT_FILLED' 'Quotes'}}
                </label>
            </span>
        </div>
        <div class="legend-help-text">
            {{str 'LBL_CONFIG_LEGEND_HELP_TEXT' 'Quotes'}}
        </div>
    </div>
    <div class="field-list">
        <span class="span6 column-1">
            {{#each fieldsListLeft}}
                {{field ../this model=../model template='detail'}}
            {{/each}}
        </span>
        <span class="span6 column-2">
            {{#each fieldsListRight}}
                {{field ../this model=../model template='detail'}}
            {{/each}}
        </span>
    </div>
</div>
