<div class="accordion-heading">
    <a id='{{name}}Title' class="accordion-toggle"
       data-help-id="{{name}}"
       data-toggle="collapse"
       data-parent="#config-accordion"
       href="#{{name}}Collapse">
    </a>
</div>
<div id="{{name}}Collapse" class="accordion-body collapse">
    <div class="accordion-inner">
        {{#each meta.panels}}
            <ol>
                {{#each fields}}
                    <li>
                        <p>{{{str label ../../module}}}</p>
                        <p>{{field ../../this model=../../model}}<p>
                    </li>
                {{/each}}
            </ol>
        {{/each}}
    </div>
</div>
