<link href="custom/include/css/survey_css/report.css" rel="stylesheet"/>
<style>
.new-page,.textbox,.commentbox,.multiselectlist,.radio-button,.check-box,.dropdownlist,.contact-information,.rating-survey,.scale,.matrix,.survey-image,.survey-video,.date-time,.doc-attachment,.boolean,.additional-text,.richtextareabox,.netpromoterscore,.emojis{
    border: 3px solid #747474;
    border-radius: 5px;
    padding-left: 10px;
    padding:5px;
    background-color: white;
    cursor:move;
}
.question{
    border: 1px solid #d5d5d5;
    border-radius: 10px;
    min-height: 100px;
    margin: 5px;
    padding: 3px;
    font-size: 11px;
    color: #747474;
    display: block;
}
.error-custom input{
    border:1px solid red;
}
.general_options .row .fa:before{
    background: #0f7799;
    padding: 5px 7px;
    color: #fff;
    font-size: 15px;
    border-radius: 3px;
}
.general_options .row .fa-pencil::before{
    background-color: transparent !important;
    color: #717171 !important;
    font-size: 13px !important;
    padding: 0 !important;
}
.general_options .row .fa-exclamation-circle::before{
    background: none !important;
    color: red !important;
    padding: 0 !important;
    font-size: 12px !important;
}
.general_options .row .fa-info-circle::before{
    background: #0f7799 !important;
    padding: 5px 7px !important;
    color: #fff !important;
    font-size: 15px !important;
    border-radius: 3px !important;
}
.general_options .row .span11 .btn > .fa:before,
.general_options .row .span5 .fa-times::before,
.general_options .row .span5 .fa-plus::before{
    background: none !important;
    padding: 0 !important;
    color: #717171 !important;
    font-size: 14px !important;
}
.row{
    margin: 10px;
}
.btn-icon i {
    color: #fff;
    margin: 8px;
    font-size: 16px;
}
.btn-icon {
    background-color: #0f7799;
    color: white;
    width: 30px;
    height: 30px;
    display: inline-block;
    vertical-align: top;
    border-radius: 3px;
}
.accordion-inner{
   background-color: #fff;
   border-bottom:1px solid #ccc; 
}
.accordion .accordion-heading{
    border-bottom : 1px solid #ccc;
    height:60px;
}

.edit_que-type option{
    padding: 3px;
}
@media only screen and (min-width:0 ) and (max-width: 767px){

    .btn-toolbar .btn.remove_page{
        font-size: 0;
        padding: 3px 5px 2px;
    }
    .row > [class*="span"], .container-fluid .row-fluid > [class*="span"]{
        margin:0px 1px 10px 0px;
    }
    .accordion-inner .row{
        float: left;
        width: 100%;
    }
    .row > .span12{
        float: none;
    }
    #custom_theme.accordion-heading{
        display: none;
    }
    .general_options .row .span5{
        margin-left: 22px;
    }
    .general_options .row .span11{
        margin-left: 0;
    }
    .span8.que-close{
        float: right;
    }
}
[data-name=survey_logo]{
    margin-bottom:3px;
}

/*Changes for Page Title*/
.error-custom input::placeholder{
    color: red;
}
</style>


<div class="create-survey">
                     
<div class="dashlet-tabs tab3 tabbable nav nav-tabs"> 
     <div class="dashlet-tabs-row">   
