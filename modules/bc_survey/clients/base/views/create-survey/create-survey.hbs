<style>
.new-page,.textbox,.commentbox,.multiselectlist,.radio-button,.check-box,.dropdownlist,.contact-information,.rating-survey,.scale,.matrix,.survey-image,.survey-video,.date-time,.doc-attachment,.boolean,.additional-text,.richtextareabox,.netpromoterscore,.emojis{
    border: 3px solid #747474;
    border-radius: 5px;
    padding-left: 10px;
    padding:5px;
    background-color: white;
    cursor:move;
}
.question{
    border: 1px solid #d5d5d5;
    border-radius: 10px;
    min-height: 100px;
    margin: 5px;
    padding: 3px;
    font-size: 11px;
    color: #747474;
    display: block;
}
.error-custom input:not('.score_weight'){
    border:1px solid red;
}
.general_options .row .fa:before{
    background: #0f7799;
    padding: 5px 7px;
    color: #fff;
    font-size: 15px;
    border-radius: 3px;
}
.general_options .row .fa-exclamation-circle::before{
    background: none !important;
    color: red !important;
    padding: 0 !important;
    font-size: 12px !important;
}
.general_options .row .fa-info-circle::before{
    background: #0f7799 !important;
    padding: 5px 7px !important;
    color: #fff !important;
    font-size: 15px !important;
    border-radius: 3px !important;
}
.general_options .row .fa-pencil::before{
    background-color: transparent !important;
    color: #717171 !important;
    font-size: 13px !important;
    padding: 0 !important;
}
.general_options .row .span11 .btn > .fa:before,
.general_options .row .span5 .fa-times::before,
.general_options .row .span5 .fa-plus::before{
    background: none !important;
    padding: 0 !important;
    color: #717171 !important;
    font-size: 14px !important;
}
.row{
    margin: 10px;
}
.btn-icon i {
    color: #fff;
    margin: 8px;
    font-size: 16px;
}
.btn-icon {
    background-color: #0f7799;
    color: white;
    width: 30px;
    height: 30px;
    display: inline-block;
    vertical-align: top;
    border-radius: 3px;
}
.accordion-inner{
   background-color: #fff;
   border-bottom:1px solid #ccc; 
}
.accordion .accordion-heading{
    border-bottom : 1px solid #ccc;
    height:60px;
}
.theme_selection{
    margin-top:5px;
    display: inline-block;
}
.SurveyTheme .theme-radio,
.SurveyTheme .theme-label{
    display: inline-block;
}
.SurveyTheme label{
    border-radius:3px;
    padding : 3px;
}
.accordion-inner .theme_selection .SurveyTheme {
    width: 24%;
    padding: 2px;
    margin: 0 !important;
    float: left;
}
.accordion-inner .theme_selection .SurveyTheme > label{
    padding:0px;
    color:#fff;
    height: 70px;
    width: auto;
    max-width: 100%;
    background-size: cover !important;
    /*border: 1px solid #e1e1e1;*/
}

img.zoom{
    visibility: hidden;
    scale :0;
    opacity: 0;
    position: absolute;
    width: auto;
    height: auto;
    left: 28px;
    top: 500px;
    border: 7px solid #fff;
    border-radius: 5px;
    transition: all 1s ease;
        -moz-transition: all 1s ease;
        -webkit-transition: all 1s ease;
        -o-transition: all 1s ease;
        -ms-transition: all 1s ease;
}
.accordion-inner .theme_selection .SurveyTheme > label:hover img.zoom{
    visibility: visible;
    scale:1;
    opacity: 1;

}
.edit_que-type option{
    padding: 3px;
}
@media only screen and (min-width:1120px) and (max-width: 1460px){
    
    .accordion-inner .theme_selection .SurveyTheme{
        width: 32%;
    }
    img.zoom{top: 600px;width: 358px;left: 19px;}
}
@media only screen and (min-width:1024px) and (max-width: 1119px){

     .accordion-inner .theme_selection .SurveyTheme{
        width: 48%;
    }
    img.zoom{top: 831px;width: 300px;left: 19px;}

}
@media only screen and (min-width:769px) and (max-width: 1023px){
    .accordion-inner .theme_selection .SurveyTheme{
        width: 48%;
    }
    img.zoom{top: 850px;width: 300px;left: 15px;}
}

@media only screen and (min-width:0 ) and (max-width: 767px){

    .btn-toolbar .btn.remove_page{
        font-size: 0;
        padding: 3px 5px 2px;
    }
    .row > [class*="span"], .container-fluid .row-fluid > [class*="span"]{
        margin:0px 1px 10px 0px;
    }
    .accordion-inner .row{
        float: left;
        width: 100%;
    }
    .row > .span12{
        float: none;
    }
    #custom_theme.accordion-heading{
        display: none;
    }
    .general_options .row .span5{
        margin-left: 22px;
    }
    .general_options .row .span11{
        margin-left: 0;
    }
    .span8.que-close{
        float: right;
    }
}
 .logic-data-table th{
    text-align: left;
}
.logic-data-table .ms-search,
.logic-data-table .ms-search input{
    width: 100% !important; 
}
.logic-data-table .logic-table-first-column{
    width: 150px;
    max-width: 150px !important;
    word-wrap: break-word;
}
.logic-data-table .logic-page-select{
    width: 302px !important;
    max-width: 302px;
    cursor: pointer;
}
.logic-data-table .logic-url-box{
    width: 291px !important;
    max-width: 291px;
}
.logic-data-table .logic-page-select option{
    word-wrap: break-word;
}
[data-name=survey_logo],[data-name=survey_background_image]{
    margin-bottom:3px;
}

/*Changes for Page Title*/
.error-placeholder input::placeholder{
    color: red;
}
.dashlet-tabs .dashlet-tabs-row .dashlet-tab:hover{
    cursor: pointer;
    color: #0f7799;
background: transparent !important;
}
.general_options .row .fa-lock::before{
    background: none !important;
    padding: 0px 0px !important;
    font-size: 11px !important;
    border-radius: 0px !important;
    color: red !important;
}
</style>


