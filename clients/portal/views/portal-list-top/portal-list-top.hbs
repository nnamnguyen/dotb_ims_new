<div class="headerpane">
    <h1><a href='#{{module}}'>{{getModuleName module plural=true}}</a></h1>
    <div class="btn-toolbar pull-right dropdown">
        {{#if context.attributes.isCreateEnabled}}
            <a href="#{{buildRoute context=context model=model action='create'}}" title='{{str "LNK_CREATE" module}}' class="btn btn-invisible"><i class="far fa-plus"></i></a>
        {{/if}}
        <a href="javascript:void(0);" class="btn btn-invisible search active"><i class="fa fa-search"></i></a>
    </div>
</div>
