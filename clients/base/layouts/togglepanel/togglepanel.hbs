<div class="search-filter">
    <div class="control-group">
        <div class="filter controls controls-two btn-group-fit">
            <div class="toggle-actions actions">
                <div class="btn-group" data-toggle="buttons-radio">
                    {{#each toggles}}
                        <button type="button" class="btn {{#eq ../options.meta.default toggle}}active{{/eq}}" data-view="{{toggle}}" data-mode="view" rel="tooltip" title="{{str title}}" aria-label="{{str title}}" aria-pressed="{{#eq ../options.meta.default toggle}}true{{else}}false{{/eq}}"><i class="fa {{class}}"></i></button>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>
<div class="main-content"></div>
