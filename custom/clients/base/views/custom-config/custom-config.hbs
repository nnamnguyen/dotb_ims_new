<div class="record tab-layout">
    <div class="row-fluid record-panel" data-panelname="panel_body">
        <div class="">
            <div class="row-fluid record-panel-content ">

                {{#each meta.fields}}
                    <div class="row-fluid panel_body panel_body">
                        <div class="span12 record-cell" data-type="url" data-name="website">
                            <div class="span3">
                                <div class="record-label" data-name="{{name}}">
                                    {{#if label}}{{str label ../this.module}}{{/if}}
                                </div>
                            </div>
                            <div class="span9">
                                <span class="normal index" data-fieldname="website" data-index="">
                                    {{field ../this model=../this.model template=../this.action}}
                                </span>
                            </div>
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>